{
    "name": "css-global-properties",
    "version": "0.2.0",
    "description": "Framework agnostic management for CSS properties (variables)",
    "keywords": [
        "css",
        "css vars",
        "css variables",
        "css properties",
        "css global properties"
    ],
    "repository": "daniil4udo/css-global-properties",
    "license": "MIT",
    "author": "Daniil Chumachenko <daniil4udo@gmail.com>",
    "type": "module",
    "exports": {
        ".": {
            "require": "./dist/index.cjs",
            "import": "./dist/index.mjs",
            "types": "./dist/index.d.ts"
        }
    },
    "main": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "rimraf dist && unbuild",
        "lint": "eslint --ext .vue,.ts,.js .",
        "prepublishOnly": "pnpm build",
        "release": "bumpp --commit --tag --no-push && pnpm publish --access public",
        "test": "vitest",
        "test:coverage": "vitest run --coverage"
    },
    "dependencies": {
        "@democrance/utils": "^0.9.1"
    },
    "devDependencies": {
        "@democrance/eslint-config": "^0.5.1",
        "@types/node": "^18.16.3",
        "@vitest/coverage-c8": "^0.30.1",
        "@vue/tsconfig": "^0.3.2",
        "bumpp": "^9.1.0",
        "eslint": "^8.39.0",
        "typescript": "^5.0.4",
        "unbuild": "^1.2.1",
        "vitest": "^0.30.1"
    },
    "packageManager": "pnpm@8.3.0",
    "engines": {
        "node": ">=14"
    }
}
