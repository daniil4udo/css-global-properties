{
    "name": "css-global-properties",
    "version": "0.3.0",
    "description": "Framework agnostic management for CSS properties (variables)",
    "keywords": [
        "css",
        "css vars",
        "css variables",
        "css properties",
        "css global properties"
    ],
    "repository": "daniil4udo/css-global-properties",
    "license": "MIT",
    "author": "Daniil Chumachenko <daniil4udo@gmail.com>",
    "type": "module",
    "exports": {
        ".": {
            "require": "./dist/index.cjs",
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "rimraf dist && tsup",
        "lint": "eslint --fix --ext .ts,.js .",
        "prepublishOnly": "pnpm build",
        "release": "bumpp --commit --tag --no-push && pnpm publish --access public",
        "test": "vitest",
        "test:coverage": "vitest run --coverage"
    },
    "devDependencies": {
        "@democrance/eslint-config": "^0.8.0",
        "@types/node": "^18.16.19",
        "@vitest/coverage-c8": "^0.33.0",
        "@vue/tsconfig": "^0.4.0",
        "bumpp": "^9.1.1",
        "eslint": "^8.44.0",
        "jsdom": "^22.1.0",
        "tsup": "^7.1.0",
        "typescript": "^5.1.6",
        "vitest": "^0.33.0"
    },
    "packageManager": "pnpm@8.3.0",
    "engines": {
        "node": ">=14"
    },
    "dependencies": {
        "@democrance/utils": "^0.14.2"
    }
}
